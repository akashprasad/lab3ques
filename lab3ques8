# lab3ques8
#include <iostream>
using namespace std;
int main()
{
    char ch;
    cout<<"enter any alphabet :\n";
    cin>>ch;
    if(ch=='a' || ch=='e' ||  ch=='i' || ch=='o'|| ch=='u')
    {
    cout<<"alphabet entered is vowel :\n";
    }
    else
    {
    cout<<"alphabet entered is consonant :\n";
    }
    return 0;
}
