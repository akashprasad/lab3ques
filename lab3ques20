# lab3ques20
#include <iostream>
using namespace std;
int main()
{
    float a,b,c;
    cout<<"enter the sides of a triangle :\n";
    cin>>a>>b>>c;
    if(a==b && b==c && c==a)
    {
    cout<<"the triangle is equilateral triangle :\n";
    }
    else if(a==b || b==c || c==a)
    {
    cout<<"the triangle is isoselece triangle :\n";
    }
    else if(a*a==b*b+c*c || b*b=c*c+a*a || c*c=a*a=b*b)
    }
    else
    {
    cout<<"the triangle is scalence triangle :\n";
    }
    return ()
}



