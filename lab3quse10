# lab3ques10
#include <iostream>
using namespace std;
int main()
{
    char ch;
    cout<<"enter any alphabet :\n";
    cin>>ch;
    if((ch>=65) && (ch<=90))
    {
    cout<<"alphabet entered is uppercase :\n";
    }
    else
    {
    cout<<"alphabet entered is lowercase :\n";
    }
    return 0;
}
